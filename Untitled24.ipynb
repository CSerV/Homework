    "def pkq(m,n,k):\n",
    "        i=m\n",
    "        total=0\n",
    "        while i<=n:\n",
    "            total=total+i\n",
    "            i=i+k\n",
    "        return total\n",
    "m=int(input('请输入开始数字:'))\n",
    "n=int(input('请输入结束数字:')) \n",
    "k=int(input('请输入间隔数字:')) \n",
    "pkq(m,n,k)  "
   ]
