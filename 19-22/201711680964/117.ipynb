    "import math,random\n",
    "def sqr(m,n,k):\n",
    "    i=0\n",
    "    summ=0\n",
    "    while i<m:\n",
    "        i=i+1\n",
    "        a=random.randint(n,k)\n",
    "        summ=summ+a\n",
    "        print(a)\n",
    "    b=math.sqrt(summ)\n",
    "    print(b)\n",
    "m=int(input(\"请输入一个整数:\"))\n",
    "n=int(input(\"请输入一个整数:\"))\n",
    "k=int(input(\"请输入一个整数:\"))\n",
    "sqr(m,n,k)"
